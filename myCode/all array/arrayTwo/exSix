<html>

<body>
  <script>
    'use strict';
    const vasya = { name: "Вася", age: 23 };
    const masha = { name: "Маша", age: 18 };
    const vovochka = { name: "Вовочка", age: 6 };

    const people = [ vasya , masha , vovochka ];

    function compareNumeric(a, b) {
      return a.age - b.age; //указываем при сортировке массива конкретный элимент объекта .age
    };

    people.sort(compareNumeric);

    // теперь people: [vovochka, masha, vasya]

    for (let i = 0; i < people.length; i++) {
      console.log(people[i].age);
    };

  </script>
</body>

</html>
